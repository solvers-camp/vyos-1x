<?xml version="1.0"?>
<interfaceDefinition>
  <node name="clear">
    <children>
      <leafNode name="log">
        <properties>
          <help>Clear contents of current master log file</help>
        </properties>
        <command>sudo journalctl --rotate --vacuum-time=1s</command>
      </leafNode>
    </children>
  </node>
</interfaceDefinition>
