### generated by service_webproxy.py ###
dbhome {{ squidguard_db_dir }}
dest {{ category }}-{{ rule }} {
{% if list_type == 'domains' %}
    domainlist      {{ category }}/domains
{% elif list_type == 'urls' %}
    urllist         {{ category }}/urls
{% elif list_type == 'expressions' %}
    expressionlist  {{ category }}/expressions
{% endif %}
}

acl {
    default {
        pass all
    }
}
